    "carbon-components-svelte": "^0.87.0",
    "carbon-icons-svelte": "^12.13.0",
    "carbon-preprocess-svelte": "^0.11.9",
    "cross-env": "^7.0.3",
    "lodash-es": "^4.17.21",
    "svelte": "^5.10.0",
    "svelte-preprocess": "^6.0.3",
    "vite": "^6.0.3"
  }
}
